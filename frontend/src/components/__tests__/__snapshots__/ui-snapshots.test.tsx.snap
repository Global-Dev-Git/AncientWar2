      </p>
      <label
        class="action-modal__field"
      >
        <span>
          Source Territory
        </span>
        bound HTMLSelectElement {
          "0": <option
            value=""
          >
            Select territory
          </option>,
          "1": <option
            value="rome_latium"
          >
            Latium
          </option>,
          "2": <option
            value="rome_etruria"
          >
            Etruria
          </option>,
        }
      </label>
      <label
        class="action-modal__field"
      >
        <span>
          Target Territory
        </span>
        bound HTMLSelectElement {
          "0": <option
            value=""
          >
            Select target
          </option>,
          "1": <option
            value="rome_etruria"
          >
            Etruria — Friendly
          </option>,
          "2": <option
            value="carthage_carthage"
          >
            Carthage — Enemy
          </option>,
        }
      </label>
      <footer>
        <button
          class="secondary"
          type="button"
        >
          Cancel
        </button>
        <button
          disabled=""
          type="button"
        >
          Confirm
        </button>
      </footer>
    </div>
  </div>
</DocumentFragment>
`;

